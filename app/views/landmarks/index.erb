<h2>All Historical Landmarks</h2>

<table>
    <th>
        Landmark
    </th>
    <th>
        Year Completed
    </th>
<% Landmark.all.each do |landmark| %>
    <tr>
        <td>
            <a href='/landmarks/<%= landmark.id %>'><%= landmark.name %></a>
        </td>
        <td>
            <%= landmark.year_completed %>
        </td>
    </tr>
<% end %>
</table>

<a class="button" href="/figures">Figures</a>
<a class="button" href="/">Home</a>
